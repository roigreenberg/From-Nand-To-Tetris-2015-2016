/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=an);
    Not(in=b, out=bn);
    And(a=a, b=bn, out=c1);
    And(a=an, b=b, out=c2);
    Or(a=c1, b=c2, out=out);
}
